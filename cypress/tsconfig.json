{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["DOM", "ESNext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["cypress"],
    "allowJs": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.js"]
}
